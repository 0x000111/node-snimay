<!DOCTYPE HTML>
<html>

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{website.title}}</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="{{website.description}}" />
	<meta name="keywords" content="{{website.keywords}}" />
	<meta name="author" content="{{website.author}}" />

	<!-- Animate.css -->
	<link rel="stylesheet" href="/css/animate.css">
	<!-- Icomoon Icon Fonts-->
	<link rel="stylesheet" href="/css/icomoon.css">
	<!-- Bootstrap  -->
	<link rel="stylesheet" href="/css/bootstrap.css">

	<!-- Flexslider  -->
	<link rel="stylesheet" href="/css/flexslider.css">

	<!-- Owl Carousel  -->
	<link rel="stylesheet" href="/css/owl.carousel.min.css">
	<link rel="stylesheet" href="/css/owl.theme.default.min.css">

	<!-- Theme style  -->
	<link rel="stylesheet" href="/css/style.css">

	<!-- Modernizr JS -->
	<script src="/js/modernizr-2.6.2.min.js"></script>
	<!-- FOR IE9 below -->
	<!--[if lt IE 9]>
	<script src="/js/respond.min.js"></script>
	<![endif]-->

	</script>
</head>

<body>

	<div class="fh5co-loader"></div>

	<div id="pages">
		<nav class="fh5co-nav" role="navigation">
			<div class="container">
				<div class="row">
					<div class="col-md-3 col-xs-6 col-sm-2">
						<div id="fh5co-logo"><a href="/">{{website.title}}.</a></div>
					</div>
					<div class="col-md-6 col-xs-6 text-center menu-1">
						<ul>
							{{#each category}}
							<li>
								<a href="/{{tag}}">{{title}}</a>
							</li>
							{{/each}} {{!--
							<li class="has-dropdown">
								<a href="/product">产品体验</a>
								<ul class="dropdown">
									<li><a href="#">卧房</a></li>
									<li><a href="#">客厅</a></li>
									<li><a href="#">餐厅</a></li>
									<li><a href="#">书房</a></li>
									<li><a href="#">儿童房</a></li>
								</ul>
							</li>--}}
							<li>
								<a href="/sales">专题活动</a>
							</li>
							<li>
								<a href="/contact">联系我们</a>
							</li>
							<li>
								<a href="/about">关于我们</a>
							</li>
						</ul>
					</div>
					<div class="col-md-3 col-xs-4 text-right hidden-xs menu-2">
						<ul>
							<li class="search">
								<div class="input-group">
									<input type="text" placeholder="Search..">
									<span class="input-group-btn">
						        <button class="btn btn-primary" type="button"><i class="icon-search"></i></button>
						      </span>
								</div>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</nav>
		{{{body}}}
		<div id="fh5co-started">
			<div class="container">
				<div class="row animate-box">
					<div class="col-md-8 col-md-offset-2 text-center fh5co-heading">
						<h2>专业设计师</h2>
						<p>优秀设计师团队，为你打造专属的家</p>
					</div>
				</div>
				<div class="row animate-box">
					<div class="col-md-8 col-md-offset-2">
						<form class="form-inline">
							<div class="col-md-4 col-sm-4">
								<div class="form-group">
									<label for="truename" class="sr-only">姓名</label>
									<input type="text" class="form-control" id="truename" placeholder="姓名">
								</div>
							</div>
							<div class="col-md-4 col-sm-4">
								<div class="form-group">
									<label for="tel" class="sr-only">联系方式</label>
									<input type="text" class="form-control" id="tel" placeholder="联系方式">
								</div>
							</div>
							<div class="col-md-4 col-sm-4">
								<button type="submit" class="btn btn-default btn-block">免费咨询</button>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
		<footer id="fh5co-footer" role="contentinfo">
			<div class="container">
				<div class="row row-pb-md">
					<div class="col-md-6 fh5co-widget">
						<h3>{{website.title}}.</h3>
						<p>{{website.companyDesc}}</p>
					</div>
					<div class="col-md-3 col-sm-6 col-xs-6 col-md-push-1">
						<ul class="fh5co-footer-links">
							<li><a href="#">联系我们</a></li>
							<li><a href="#">关于我们</a></li>
							<li><a href="#">公司简介</a></li>
							<li><a href="#">品牌荣誉</a></li>
						</ul>
					</div>

					<div class="col-md-3 col-sm-6 col-xs-6 col-md-push-1">
						<ul class="fh5co-footer-links">
							{{#each category}}
							<li>
								<a href="/{{tag}}">{{title}}</a>
							</li>
							{{/each}}
							<li><a href="#">专题活动</a></li>
						</ul>
					</div>
				</div>

				<div class="row copyright">
					<div class="col-md-12 text-center">
						<p>
							<h2><small class="block">{{website.address}}</small> </h2>
						</p>
						<p>
							<small class="block">{{website.copyright}}</small>
						</p>
						<p>
							<small class="block">{{website.icp}}</small>
						</p>
						{{!--
						<p>
							<ul class="fh5co-social-icons">
								<li><a href="#"><i class="icon-twitter"></i></a></li>
								<li><a href="#"><i class="icon-facebook"></i></a></li>
								<li><a href="#"><i class="icon-linkedin"></i></a></li>
								<li><a href="#"><i class="icon-dribbble"></i></a></li>
							</ul>
						</p>--}}
					</div>
				</div>

			</div>
		</footer>
	</div>

	<div class="gototop js-top">
		<a href="#" class="js-gotop"><i class="icon-arrow-up"></i></a>
	</div>

	<!-- jQuery -->
	<script src="/js/jquery.min.js"></script>
	<!-- jQuery Easing -->
	<script src="/js/jquery.easing.1.3.js"></script>
	<!-- Bootstrap -->
	<script src="/js/bootstrap.min.js"></script>
	<!-- Waypoints -->
	<script src="/js/jquery.waypoints.min.js"></script>
	<!-- jQuery query-object-->
	<script src="/js/jquery.query-object.js"></script>
	<!-- Carousel -->
	<script src="/js/owl.carousel.min.js"></script>
	<!-- countTo -->
	<script src="/js/jquery.countTo.js"></script>
	<!-- Flexslider -->
	<script src="/js/jquery.flexslider-min.js"></script>
	<!-- Main -->
	<script src="/js/main.js"></script>
	<!-- bootstrap-paginator -->
	<script src="/js/bootstrap-paginator.min.js"></script>

	{{{_sections.pagination}}}
</body>

</html>